		"prefix": "ejs.addClass",
		"body": [
			"ejs.addClass(element, className);"
		],
		"description": "Pass in element (by ID) and the class name. Remember Quotes!"
	},
	"Remove Class":{
		"prefix": "ejs.removeClass",
		"body": [
			"ejs.removeClass(element, className);"
		],
		"description": "Pass in element (by ID) and the class name. Remember Quotes!"
	},
